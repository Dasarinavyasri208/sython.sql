Q1 = "SELECT FNAME, LNAME FROM ACTOR INNER JOIN CAST ON ID = PID WHERE MID = 12148;"

Q2 = "SELECT COUNT(MID) FROM CAST INNER JOIN ACTOR ON ID = PID WHERE FNAME = 'Harrison (I)' AND LNAME = 'Ford';"

Q3 = "SELECT DISTINCT (PID) FROM CAST JOIN MOVIE ON ID = MID WHERE NAME LIKE 'YOUNG LATIN GIRLS%';"

Q4 = "SELECT COUNT(DISTINCT(PID)) FROM CAST JOIN MOVIE ON ID = MID WHERE YEAR BETWEEN 1990 AND 2000 ;"
